{
  "name": "html-biscuits",
  "private": "true",
  "publisher": "CodeBiscuits",
  "displayName": "HTML Biscuits",
  "repository": "https://github.com/code-biscuits/html-biscuits",
  "description": "Add annotations, hereso unto for known as \"Biscuits\", to the end of the line of closing html tags. Such Biscuits could include ids and /or class names. Make div soup digestible!",
  "icon": "biscuits-styled.png",
  "version": "0.0.8",
  "license": "MIT",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "HTML",
    "hints",
    "readability",
    "annotation",
    "biscuits"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./bundled/extension.js",
  "contributes": {
    "commands": [],
    "configuration": {
      "properties": {}
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile && yarn run bundle",
    "compile": "tsc -b ./",
    "bundle:extension": "rollup ./out/extension.js --file ./bundled/extension.js --format cjs",
    "bundle:activate": "rollup ./out/activate.js --file ./bundled/activate.js --format cjs",
    "bundle": "yarn run bundle:activate && yarn run bundle:extension",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "rollup": "^2.28.2",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "peerDependencies": {},
  "dependencies": {
    "vscode-html-languageservice": "^3.1.3",
    "vscode-languageclient": "^6.1.3"
  }
}
